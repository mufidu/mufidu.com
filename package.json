{
  "name": "eleventy-high-performance-blog",
  "version": "5.0.2",
  "license": "MIT",
  "description": "A starter repository for a blog web site using the Eleventy static site generator.",
  "scripts": {
    "watch:sass": "npx sass --watch --style=compressed sass/main.scss css/main.css",
    "build:sass": "npx sass --style=compressed sass/main.scss css/main.css",
    "build": "npm run js-build && npm run build:sass && eleventy",
    "start": "concurrently \"npm run watch:sass\" \"eleventy --serve\" \"npm run js-build-watch\"",
    "serve": "DEBUG=Eleventy:Benchmark* npx @11ty/eleventy --serve",
    "js-build": "rollup -c rollup.config.js",
    "js-build-watch": "rollup -c rollup.config.js -w",
    "debug": "DEBUG=* eleventy && npm run test",
    "test": "mocha test/test*.js",
    "test-watch": "mocha test/test*.js --watch"
  },
  "pre-push": [
    "build"
  ],
  "pre-commit": [],
  "repository": {
    "type": "git",
    "url": "git://github.com/11ty/eleventy-base-blog.git"
  },
  "bugs": {
    "url": "https://github.com/11ty/eleventy-base-blog/issues"
  },
  "homepage": "https://github.com/11ty/eleventy-base-blog#readme",
  "devDependencies": {
    "@11ty/eleventy": "^0.12.1",
    "@11ty/eleventy-navigation": "^0.1.3",
    "@11ty/eleventy-plugin-rss": "^1.0.7",
    "@11ty/eleventy-plugin-syntaxhighlight": "^3.0.1",
    "@rollup/plugin-node-resolve": "^13.0.4",
    "eleventy-plugin-local-images": "^0.4.0",
    "file-type": "^12.0.1",
    "fs-extra": "^8.1.0",
    "jsdom": "^15.1.1",
    "luxon": "^1.21.3",
    "markdown-it": "^8.4.2",
    "markdown-it-anchor": "^5.2.5",
    "node-fetch": "^2.6.1",
    "pre-commit": "^1.2.2",
    "pre-push": "^0.1.1",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-terser": "^6.1.0",
    "sass": "^1.39.0",
    "shorthash": "0.0.2"
  },
  "dependencies": {
    "@ampproject/toolbox-optimizer": "^2.8.3",
    "any-shell-escape": "^0.1.1",
    "clean-css": "^4.2.3",
    "commentbox.io": "^2.1.0",
    "concurrently": "^5.2.0",
    "csp-hash-generator": "0.0.3",
    "csso": "^4.0.3",
    "datauri": "^3.0.0",
    "eleventy-plugin-embed-everything": "^1.13.3",
    "eleventy-plugin-youtube-embed": "^1.6.4",
    "expect.js": "^0.3.1",
    "ffmpeg-static": "^4.2.7",
    "hasha": "^5.2.0",
    "html-minifier": "^4.0.0",
    "image-size": "^0.8.3",
    "jimp": "^0.16.1",
    "lru-cache": "^5.1.1",
    "mocha": "^8.1.3",
    "phin": "^3.5.0",
    "purge-from-html": "^1.0.3",
    "purgecss": "^4.0.3",
    "querystring": "^0.2.0",
    "rollup": "^2.23.0",
    "sharp": "^0.29.0",
    "twitter": "^1.7.1"
  }
}
